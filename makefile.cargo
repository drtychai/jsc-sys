all:
	cd ${OUT_DIR}/build && \
	cmake -DPORT="JSCOnly" \
		--build ${OUT_DIR}/build \
		-GNinja \
		-DCMAKE_MAKE_PROGRAM=ninja \
		-DENABLE_STATIC_JSC=ON \
		-DCMAKE_EXPORT_COMPILE_COMMANDS=OFF \
		-DDEVELOPER_MODE=OFF \
		-DENABLE_EXPERIMENTAL_FEATURES=OFF \
		-DUSE_THIN_ARCHIVES=OFF \
	"${CARGO_MANIFEST_DIR}/WebKit" && \
	ninja -C ${OUT_DIR}/build

clean:
	cd WebKit && make clean
